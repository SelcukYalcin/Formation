{% extends 'base.html.twig' %}

{% block title %}SESSIONS{% endblock %}

{% block body %}

<h1>SESSIONS</h1>

<div class="example-wrapper">
    <table>
        <thead>
            <tr>
                <th>INTITULE</th>     
            </tr>
            
        </thead>
        <tbody>
            {% for session in sessions %}
                <a href="{{ path('show_session', {'id': session.id}) }}">{{ session }}  </a><br>
            {% endfor %}
        </tbody>
    </table>
</div>
{% endblock %}
